<odoo>
    <data>
        <record id="view_res_bank_inherit" model="ir.ui.view">
            <field name="name">res.bank.inherit</field>
            <field name="model">res.bank</field>
            <field name="inherit_id" ref="base.view_res_bank_form"/>
            <field name="arch" type="xml">
              <xpath expr="//form/sheet/group/group[@name='communication_details']" position="after">                
                <group>
                    <field name="template_extractos"/>
                </group>
              </xpath>
            </field>
        </record>
    </data>
</odoo>